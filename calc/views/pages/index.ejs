</div>
<div id="all-cost">
    <div class="all-cost_title">
        <span>Всего, цена с доставкой</span>
        <span>Для получения общей суммы, внесите данные в Таможенный калькулятор и нажмитe "Расчитать"</span>
    </div>
    <span id="main-cost">$ 0</span>
</div>
<div id="preloader">
    <b>Загрузка...</b>
</div>
<script>
    const fetchPromise = fetch("/getCalcData");
    fetchPromise.then(response => {
        return response.json();
    }).then(result => {

        const preloader = document.querySelector('#preloader');
        preloader.style.display = 'none';

        const consts = result[6][0];

        const selectMark = document.getElementById("mark");
        const selectModel = document.getElementById("modеl");
        const selectYear = document.getElementById("year");
        const lotInput = document.getElementById("lotInput");
        const buttonLot = document.getElementById("calculateLot");
        const lotResultWrapper = document.getElementById("lotResult");
        const buttonAverage = document.getElementById("calculate_average");

        const minAverage = document.getElementById("min-value__average");
        const mediumAverage = document.getElementById("medium-value__average");
        const maxAverage = document.getElementById("max-value__average");

        let automobiles;

        const i = "https://";
        const ii = "psm";
        const iii = "invoice";

        const calcCustoms = document.getElementById('calc-сustoms');
        const motorSizesSelect = document.getElementById('motor-size');
        const datesCreateSelect = document.getElementById('date-create');
        const ageRatioElem = document.getElementById('ageRatio');
        const carCostElem = document.getElementById('car-cost');
        const motorTypes = document.getElementsByClassName('motor-type');
        const calcBtn = document.getElementById('calculate');
        const electroSizeElem = document.getElementById('electro-size');
        const MREOCost = document.getElementById('MREOCost');
        const calcDate = document.querySelector('.calc-date');

        consts.date = consts.date ? consts.date : '1.02.21'
        calcDate.textContent = `Калькулятор доставки c ${consts.date}`;

        const certificateConstant = consts.certificate_constant;
        const portOdessa = consts.port_odessa;
        const griznes = consts.griznes;
        const copartConstant = consts.copart_constant;
        const IAAIConstant = consts.IAAI_constant;
        const SUVConst = consts.SUV_const;

        const extraChargesItem = document.querySelectorAll('.extra-charges_item__cost');
        extraChargesItem[1].textContent = `$ ${griznes}`;
        extraChargesItem[2].textContent = `$ ${portOdessa}`;
        extraChargesItem[4].textContent = `$ ${certificateConstant}`;

        const calculatorDelivery = document.getElementById('calc-delivery');
        const citySelect = document.getElementById('departure-city');
        const portSelect = document.getElementById('departure-port');
        const arrivalSelect = document.getElementById('arrival-port-select');
        const deliverySelect = document.getElementById('delivery-select');
        const SUVEl = document.getElementById('SUV');
        
        let deliveryCost = 0;

        let costCalc1 = 0;
        let auction = 'Copart';
        let finalBetValue = 0;
        let insuranceValue = 0;

        let isElectro = false;
        let EurToUsd;
        let cubeRatio = 50;
        let ageRatioVal = 1;
        let carCost = 0;
        let allCustomCost = 0;
        let MREOTax = 0;

        const citiesForDelivery = result[5];
        const CopartCitiesAndPorts = result[0];
        const IAAICitiesAndPorts = result[1];
        const CopartFinalBetRange = result[2];
        const CopartBetsInRealTime = result[4];
        const IAAIFinalBetRange = result[3];
        const IAAIBetsInRealTime = result[8];
        let acceptSeller = [];

    for(let i = 0; i < result[7].length; i++){
        acceptSeller.push(result[7][i].seller);
    }

    loadOptionToSelect();

    function calcMainCost() {
        const deliveryInUkraine = +deliverySelect.value;
        let mainCost = Number(costCalc1) + Number(deliveryCost) + Number(allCustomCost);
        mainCost += deliveryInUkraine;

        if (SUVEl.checked) {
            mainCost += SUVConst;
        }

        document.getElementById('main-cost').innerHTML = `$ ${Math.round(mainCost)}`;


    }

    function calculateMREO(sum) {
        let res = 0;
        if (12500 > sum) {
            res = (sum * 3) / 100;
        } else if (12500 < sum && sum < 19500) {
            res = (sum * 4) / 100;
        } else {
            res = (sum * 5) / 100;
        }
        res += 115;
        return res;
    }

    carCostElem.onkeypress = function (event) {
        validate(event);
        this.value = this.value.replace(/\s/g, '');
        if (this.value.slice(-1) === '$') {
            this.value = this.value.substring(0, this.value.length - 1);
        }
    }
    carCostElem.onkeyup = function () {
        var key = event.keyCode || event.charCode;
        if (key == 8 || key == 46) {
            return false;
        } else {
            this.value = this.value.replace(/\s/g, '');
            if (this.value.slice(-1) !== '$') {
                this.value = `${this.value} $`;
            }
        }
    }
    for (let i = 0; i < motorTypes.length; i += 1) {
        motorTypes[i].addEventListener('click', setMotorType);
    }

    function motorSizesChange() {
        const current = document.querySelector(".type-of-motor").querySelector(".active");
        console.log('current', current);
        if (current.id === 'gasoline') {
            if (motorSizesSelect.value > 3) {
                cubeRatio = 100;
            } else {
                cubeRatio = 50;
            }
        } else if (this.id === 'diesel') {
            cubeRatio = 75;
        }
    }

    function setMotorType() {
        removeActiveMotorTypes();
        this.classList.add('active');
        if (this.id === 'gasoline') {
            if (motorSizesSelect.value > 3) {
                cubeRatio = 100;
            } else {
                cubeRatio = 50;
            }
        } else if (this.id === 'diesel') {
            cubeRatio = 75;
        } else {
            renderElectroInput();
        }
    }

    function renderElectroInput() {
        isElectro = true;
        document.getElementById('second-row').classList.add('hide');
        document.getElementById('for-electro').classList.remove('hidden');
        document.getElementById('for-classic').classList.add('hidden');
    }

    function removeActiveMotorTypes() {
        isElectro = false;
        document.getElementById('second-row').classList.remove('hide');
        document.getElementById('for-electro').classList.add('hidden');
        document.getElementById('for-classic').classList.remove('hidden');
        for (let i = 0; i < motorTypes.length; i += 1) {
            motorTypes[i].classList.remove('active');
        }
    }

    function setCarCost(val) {
        carCost = val + 400;
        MREOTax = calculateMREO(carCost);
        carCostElem.value = val > 0? `$ ${Math.round(carCost)}`: `$ 0`;
        MREOCost.innerText = val > 0? `$ ${Math.round(MREOTax)}`: `$ 0`;
    }

    function setAgeRatio(value) {
        ageRatioVal = value;
        ageRatioElem.innerText = `Коеффициент возраста - ${ageRatioVal}`;
    }

    function loadMotorSizes() {
        let i = 1;
        while (i < 10.1) {
            const opt = document.createElement('option');
            opt.appendChild(document.createTextNode(`${i} л`));
            opt.value = i;
            motorSizesSelect.appendChild(opt);
            i = (i * 10 + 1) / 10;
        }
    }

    loadMotorSizes();

    function loadDatesCreate() {
        const currentYear = new Date().getFullYear();
        for (let i = 0; i < 21; i += 1) {
            const opt = document.createElement('option');
            const year = currentYear - i;
            opt.appendChild(document.createTextNode(year));
            opt.value = year;
            datesCreateSelect.appendChild(opt);
        }
    }

    function calcAgeRatio(year) {
        let coef;
        const currentYear = new Date().getFullYear();
        if (year <= currentYear && year >= currentYear - 2) {
            coef = 1;
        } else {
            coef = currentYear - year - 1;
        }
        return (coef > 15) ? 15 : coef;
    }

    datesCreateSelect.onchange = function () {
        setAgeRatio(calcAgeRatio(this.value));
    }

    function getExchangeRates() {
        var xhr = new XMLHttpRequest();
        xhr.open('GET', 'https://api.ratesapi.io/api/latest', false);
        xhr.send();
        if (xhr.status != 200) {
            alert(xhr.status + ': ' + xhr.statusText);
        } else {
            const data = JSON.parse(xhr.responseText);
            EurToUsd = data.rates.USD;
        }
    }

    function getUAExchange() {
        var xhr = new XMLHttpRequest();
        xhr.open('GET', 'https://api.privatbank.ua/p24api/pubinfo?json&exchange&coursid=5', false);
        xhr.send();
        if (xhr.status != 200) {
            alert(xhr.status + ': ' + xhr.statusText);
        } else {
            const data = JSON.parse(xhr.responseText);
            document.getElementById('currentExchange').innerHTML
                = `Курс на ${getDateInFormat()} - <b>${parseFloat(data[0].sale).toFixed(2)}</b>`;
        }

        function getDateInFormat() {
            const todayDate = new Date();
            const month = ((todayDate.getMonth() + 1) < 10)
                ? `0${(todayDate.getMonth() + 1)}`
                : (todayDate.getMonth() + 1);
            return `${todayDate.getDate()}.${month}.${todayDate.getFullYear()}`;
        }
    }

    calcBtn.onclick = function () {
        let allCost;
        if (!isElectro) {
            const duty = carCostElem.value.split(" ")[1] * 0.1;
            const excise = (cubeRatio * EurToUsd) * motorSizesSelect.value * ageRatioVal;
            const vat = (Number(carCostElem.value.split(" ")[1]) + duty + excise) * 0.2;
            allCost = duty + excise + vat;
        } else {
            allCost = electroSizeElem.value * 1.0 * EurToUsd;
        }
        setAllCost(allCost);
    }
    electroSizeElem.onkeypress = function (event) {
        validate(event);
    }
    electroSizeElem.onkeyup = function (event) {
        if (event.target.value < 1) {
            event.target.value = 1;
        }
    }

    function setAllCost(val) {
        allCustomCost = val + MREOTax + certificateConstant;
        calcMainCost();
        calcCustoms.getElementsByClassName('total-cost_value')[0].innerText = `$ ${Math.round(allCustomCost)}`;
    }

    const calculatorCost = document.getElementById('calc-cost');
    const finalBetInput = document.getElementById('final-bet');
    const copartAuctionsBTN = document.getElementsByClassName('CopartAuction');
    const IAAIAuctionsBTN = document.getElementsByClassName('IAAIAuction');


    calcTotalCost(0, 'Copart');
    loadOptionToSelect();
    setPorts(citySelect.value);
    motorSizesSelect.addEventListener('change', motorSizesChange);
    loadDatesCreate();
    getExchangeRates();
    getUAExchange();
    fetchCarList();


    function changeActiveAuction(add, del) {
        for (let i = 0; i < copartAuctionsBTN.length; i++) {
            del[i].classList.remove("activeAuction");
            add[i].classList.add("activeAuction");
        }
    }

    for (let i = 0; i < copartAuctionsBTN.length; i++) {
        copartAuctionsBTN[i].addEventListener('click', function () {
            auction = 'Copart';
            changeActiveAuction(copartAuctionsBTN, IAAIAuctionsBTN);
            calcTotalCost(finalBetValue, auction);
            loadOptionToSelect();
        })

        IAAIAuctionsBTN[i].addEventListener('click', function () {
            auction = 'IAAI';
            changeActiveAuction(IAAIAuctionsBTN, copartAuctionsBTN);
            calcTotalCost(finalBetValue, auction);
            loadOptionToSelect()
        })
    }

    function calculateInsurance(finalBetValue) {
        if(insurance.checked){
            insuranceValue = Math.ceil((+calculatorCost.getElementsByClassName('auction')[0].innerText.split(" ")[1] + +finalBetValue) *6 /1000);
        }else {
            insuranceValue = 0;
        }
        calculatorCost.querySelector("#insurance_cost").innerHTML = `$ ${insuranceValue}`

    }

    insurance.onclick = () => {
        calculateInsurance(finalBetInput.value);
        calcTotalCost(finalBetValue, auction);
    }

    finalBetInput.onkeyup = function () {
        finalBetValue = this.value;
        calcTotalCost(finalBetValue, auction);
        calculateInsurance(finalBetInput.value);
    }

    function count(finalBat, auctionFee) {
        let totalCost = Number(finalBat) + portOdessa + griznes + auctionFee;
        if(insurance.checked){
            totalCost += insuranceValue
        }
        costCalc1 = totalCost;
        updateInfo(auctionFee, totalCost);
        setCarCost(totalCost - portOdessa - griznes - insuranceValue);
    }

    function calcTotalCost(val, auc) {
        const finalBat = val;
        let auctionFee = 0;
        if (Number(finalBat) < 1) {
            auctionFee = 0;
        } else {
            if (auc == "Copart") {
                auctionFee = getFeeAmount(finalBat, CopartFinalBetRange) + getBetInRealTime(finalBat, CopartBetsInRealTime) + copartConstant;
            } else {
                auctionFee = getFeeAmount(finalBat, IAAIFinalBetRange) + getBetInRealTime(finalBat, IAAIBetsInRealTime) + IAAIConstant;
            }
        }
        count(finalBat, auctionFee)
    }

    finalBetInput.onkeypress = function (event) {
        validate(event);
    }

    function updateInfo(auctionFee, totalCost) {
        calculatorCost.getElementsByClassName('auction')[0].innerText = `$ ${Math.round(auctionFee)}`;
        calculatorCost.getElementsByClassName('total-cost_value')[0].innerText = `$ ${Math.round(totalCost)}`;
    }

    function validate(evt) {
        var theEvent = evt || window.event;
        // Handle paste
        if (theEvent.type === 'paste') {
            key = event.clipboardData.getData('text/plain');
        } else {
            // Handle key press
            var key = theEvent.keyCode || theEvent.which;
            key = String.fromCharCode(key);
        }
        var regex = /[0-9]|\./;
        if (!regex.test(key)) {
            theEvent.returnValue = false;
            if (theEvent.preventDefault) theEvent.preventDefault();
        }
    }

    const getBetInRealTime = (bet, arr) => {
        let amount = 0;
        for (let i = 0; i < arr.length; i += 1) {
            if (bet >= arr[i].min && bet <= arr[i].max) {
                amount = arr[i].bet;
                break;
            }
        }
        return amount;
    }
    const getAmountInPercent = (bet, percent) => {
        return ((bet * percent) / 100);
    }
    const getFeeAmount = (bet, arr) => {
        let amount = 0;
        for (let i = 0; i < arr.length; i += 1) {
            if (bet >= arr[i].min && bet <= arr[i].max) {
                let rate = arr[i].fee_amount ? arr[i].fee_amount : 0;
                amount = getAmountInPercent(bet, arr[i].feeAmountInPercent) ? rate + getAmountInPercent(bet, arr[i].feeAmountInPercent) : rate;
                break;
            }
        }
        return amount;
    }

    function getCurrentCitiesAndPortArr() {
        let currentArr = CopartCitiesAndPorts
        switch (auction) {
            case "Copart":
                currentArr = CopartCitiesAndPorts;
                break;
            case "IAAI":
                currentArr = IAAICitiesAndPorts;
                break;
        }
        return currentArr
    }

    function loadOptionToSelect() {
        let currentArr = getCurrentCitiesAndPortArr();
        citySelect.innerHTML = '';
        portSelect.innerHTML = '';
        deliverySelect.innerHTML = "";
        for (let i = 0; i < currentArr.length; i += 1) {
            const opt = document.createElement('option');
            opt.appendChild(document.createTextNode(currentArr[i].city));
            opt.value = currentArr[i].city;
            citySelect.appendChild(opt);
        }
        citiesForDelivery.forEach(data => {
            const opt = document.createElement('option');
            opt.appendChild(document.createTextNode(data.city));
            opt.value = data.cost;
            deliverySelect.appendChild(opt)
        });
        setPorts(citySelect[0].innerText);
    }

    const portSelectHandler = () => {
        setTotalCost(portSelect.value);
        if (portSelect.options[portSelect.options.selectedIndex].label == "CA") {
            arrivalSelect.innerText = "Chernomorsk"
        } else {
            arrivalSelect.innerText = "Chernomorsk"
        }
    }

    portSelect.onchange = portSelectHandler;
    deliverySelect.onchange = () => setTotalCost(deliveryCost);

    const citySelectHandler = () => {
        removeOptions();
        portSelect.classList.remove('disabled');
        setPorts(citySelect.value);
    }

    citySelect.onchange = citySelectHandler;

    function createPortOfArrival(currentCity) {
        removeOptions();
        let ports = [];
        for (let key in currentCity) {
            if (key !== 'city' && currentCity[key] !== -1) {
                ports.push({name: key, cost: currentCity[key]});
            }
        }
        for (let i = 0; i < ports.length; i += 1) {
            const opt = document.createElement('option');
            opt.appendChild(document.createTextNode(ports[i].name.toUpperCase()));
            opt.value = ports[i].cost;
            portSelect.appendChild(opt);
        }
        if (ports.length === 1) {
            portSelect.classList.add('disabled');
        }
        if (ports.length > 1) {
            portSelect.classList.remove('disabled');
        }
        if (portSelect[0] && portSelect[0].innerText == "CA") {
            arrivalSelect.innerText = "Chernomorsk"
        } else {
            arrivalSelect.innerText = "Chernomorsk"
        }
        if(ports[0]){
            // не хватает данныых, что бы это работало адекватно
            setTotalCost(ports[0].cost);
        }
    }

    function setPorts(cityName) {
        let currentArr = CopartCitiesAndPorts;
        switch (auction) {
            case "Copart":
                currentArr = CopartCitiesAndPorts;
                break;
            case "IAAI":
                currentArr = IAAICitiesAndPorts;
                break;
        }
        const currentCity = currentArr.filter(item => item.city === cityName);
        createPortOfArrival(currentCity[0])
    }

    function removeOptions() {
        portSelect.innerHTML = '';
    }

    SUVEl.onchange = () => {
        setTotalCost(deliveryCost);
    };

    function setTotalCost(cost) {
        deliveryCost = cost;
        const deliveryInUkraine = +deliverySelect.value;

        if (SUVEl.checked) {
            calculatorDelivery.getElementsByClassName('total-cost_value')[0].innerText = `$ ${+cost + SUVConst + deliveryInUkraine}`;
        } else {
            deliveryCost = cost;
            calculatorDelivery.getElementsByClassName('total-cost_value')[0].innerText = `$ ${+cost + deliveryInUkraine}`;
        }

    }

    selectMark.addEventListener("change", () => {
        renderModels(automobiles[selectMark.value]);
    });

    selectModel.addEventListener("change", () => {
        renderYear(automobiles[selectMark.value][selectModel.value]);
    });

    function fetchCarList() {
        const xhr = new XMLHttpRequest();
        xhr.open("GET", `${i}${ii}${iii}.com/car/list`, false);
        xhr.onreadystatechange = function () {
            if (xhr.readyState === XMLHttpRequest.DONE && xhr.status === 200) {
                automobiles = JSON.parse(xhr.responseText);
                for (let auto in automobiles) {
                    const optionMark = new Option(auto, auto, false, false);
                    selectMark.appendChild(optionMark);
                }
            }
        };
        xhr.send();
    }

    function renderModels(autoClister) {
        selectModel.innerHTML =
            '<option selected value="Выберите модель автомобиля">Выберите модель автомобиля</option>';
        for (let auto in autoClister) {
            const optionModel = new Option(auto, auto, false, false);
            selectModel.appendChild(optionModel);
        }
    }

    function renderYear(years) {
        selectYear.innerHTML =
            '<option selected value="Выберите год выпуска">Выберите год выпуска</option>';
        years.forEach((year) => {
            const optionModel = new Option(year, year, false, false);
            selectYear.appendChild(optionModel);
        });
    }

    function fetchCarLot() {
        const getLotNumber = lotInput.value;
        const xhr = new XMLHttpRequest();
        xhr.open("GET", `${i}${ii}${iii}.com/car?id=${getLotNumber}&auction=${auction}`, true);
        console.log(`${i}${ii}${iii}.com/car?id=${getLotNumber}&auction=${auction}`);
        xhr.setRequestHeader("content-type", "application/json; charset=utf-8");
        xhr.onreadystatechange = function () {
            if (xhr.readyState === XMLHttpRequest.DONE && xhr.status === 200) {
                const resultLot = JSON.parse(xhr.responseText);
                console.log('result lot', resultLot)
                if (auction === "Copart") {
                    showResults(resultLot[0]);
                    calculateFetchCar(resultLot[0]);
                } else {
                    showResults(resultLot);
                    calculateFetchCar(resultLot);
                }

                lotResultWrapper.style.display = "flex";
            }
        };
        xhr.send();
    }

    buttonLot.addEventListener("click", fetchCarLot);

    function showResults(resultLot) {
        let img = document.createElement("img");
        let data = document.createElement("div");
        let messRes = '';
        img.classList.add("lot-img");
        data.classList.add("lot-data");
        lotResultWrapper.innerHTML = "";
        if (!resultLot) {
            messRes = "<div><h3 class=\"lot-field\">Такой лот не найден </h3></div>"
        } else {
            const {minBA, mark, engine, fuelType, body_type, transmission, odometer, lot_num, market_value, currency, price, carImage, cylindres, status, year, sellerName} = resultLot;
            img.setAttribute("src", carImage);
            img.setAttribute("alt", "car-image");
            lotResultWrapper.appendChild(img);
            messRes = `<div>
  <h3 class="lot-field">Марка автомобиля: <p class="lot-value"> ${mark}</p></h3>
  <h3 class="lot-field">Год выпуска: <p class="lot-value"> ${year}</p></h3>
  <h3 class="lot-field">Объем двигателя: <p class="lot-value"> ${engine}</p></h3>
  <h3 class="lot-field">Количество цилиндров: <p class="lot-value"> ${cylindres}</p></h3>
  <h3 class="lot-field">Тип топлива: <p class="lot-value"> ${fuelType}</p></h3>
  <h3 class="lot-field">Тип кузова: <p class="lot-value"> ${body_type}</p></h3>
  <h3 class="lot-field">Трансмиссия: <p class="lot-value"> ${transmission}</p></h3>
  </div>
  <div>
  <h3 class="lot-field">Одометр: <p class="lot-value"> ${odometer}</p></h3>
  <h3 class="lot-field">Номер лота: <p class="lot-value"> ${lot_num}</p></h3>
  <h3 class="lot-field">Рыночная стоимость: <p class="lot-value"> ${market_value}<span class="lot-currency"> ${currency}</span></p></h3>
  <h3 class="lot-field">Ставка: <p class="lot-value"> ${price}<span class="lot-currency"> ${currency}</span> </p></h3>
  <h3 class="lot-field">Минималка продавца: <p class="lot-value"> ${minBA} </p></h3>
  <h3 class="lot-field">status: <p class="lot-value"> ${status} </p></h3>
  <h3 class="lot-field">seller: <p class="lot-value" style='color:${acceptSeller.includes(sellerName)? "color: green;": " red;"}'>${sellerName !== 0 && sellerName? sellerName : "Не указан" }</p></h3>
   </div>
   `
        }
        data.innerHTML = messRes;
        lotResultWrapper.appendChild(data);
    }

    const getNormalizeLocation = l => {
        const regexState = /\([a-zA-Z][a-zA-Z]\)/gm;
        const state = l.match(regexState)[0].slice(0, -1).slice(1);
        const city = l.replace(regexState, " ").trim().split("-").join(" ");
        const res = `${city} - ${state}`;
        return res.toUpperCase()
    };

    function calculateFetchCar(data) {
        console.log('calculateFetchCar, data', data);
        const motorType = document.querySelector(".type-of-motor").querySelectorAll(".motor-type");

        const location = auction === "Copart" ? data.location : getNormalizeLocation(data.location);

        const currentLocation = getCurrentCitiesAndPortArr();
        createPortOfArrival(currentLocation.find(item => item.city.toUpperCase() === location));

        //ставлю тип двигателя
        for (let type = 0; type < motorType.length; type++) {
            if(data.fuelType.toLowerCase() !== "other"){
                motorType[type].classList.remove('active');
                if (data.fuelType.toLowerCase()[0] === motorType[type].id[0]) {
                    motorType[type].classList.add('active');
                }
            }

        }
        if (data.fuelType.toLowerCase()[0] === "g" || data.fuelType.toLowerCase()[0] === "d") {
            // обработка дизеля и бензина
            motorSizesSelect.value = +data.engine.slice(0, 3)
        } else {
            // для електрокаров
            renderElectroInput();
            //todo добавить значиния мощности двигателя???
        }
        motorSizesChange();

        //ставлю цену
        if (data.price > data.minBA) {
            finalBetInput.value = data.price;
            calcTotalCost(data.price, auction);
        } else {
            finalBetInput.value = data.minBA;
            calcTotalCost(data.minBA, auction);
        }

        //ставлю год и коеф.
        setAgeRatio(calcAgeRatio(data.year));
        datesCreateSelect.value = data.year
    }

    function setAverage(data) {
        minAverage.innerHTML = `$ ${data[0]["AVG(price) * 0.3"]}`;
        mediumAverage.innerHTML = `$ ${data[0]["AVG(price) * 0.3"]}`;
        maxAverage.innerHTML = `$ ${data[0]["MAX(price) * 0.3"]}`;
    }

    function fetchAverage() {
        const xhr = new XMLHttpRequest();
        xhr.open("GET", `${i}${ii}${iii}.com/car?model=${selectModel.value}&mark=${selectMark.value}&year=${selectYear.value}`, true);
        xhr.setRequestHeader("content-type", "application/json; charset=utf-8");
        xhr.onreadystatechange = function () {
            if (xhr.readyState === XMLHttpRequest.DONE && xhr.status === 200) {
                const average = JSON.parse(xhr.responseText);
                average && setAverage(average)
            }
        };
        xhr.send();
    }

    buttonAverage.addEventListener('click', fetchAverage);
});
</script>
</body>
</html>
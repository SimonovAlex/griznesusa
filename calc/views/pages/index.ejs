            <span class="total-cost_text">Общая стоимость</span>
            <span class="total-cost_value">$ 0</span>
        </div>
        <div class="calculator-row">
            <div class="type-of-motor">
                <span class="column-title">Тип двигателя</span>
                <span class="motor-type active" id="gasoline">Бензин</span>
                <span class="motor-type" id="diesel">Дизель</span>
                <span class="motor-type" id="electro">Электро</span>
            </div>
            <div class="motor-size">
                <div id="for-classic">
                    <span class="column-title">Обьем двигателя</span>
                    <div class="select-wrapper">
                        <select name="motor-size" id="motor-size" >
                        </select>
                    </div>
                </div>
                <div id="for-electro" class="hidden">
                    <span class="column-title">Мощность двигателя (кВт)</span>
                    <input type="text" name="electro-size" id="electro-size" />
                </div>
            </div>
        </div>
        <div class="calculator-row" id="second-row">
            <div class="date-create">
                <span class="column-title">Год выпуска</span>
                <div class="select-wrapper">
                    <select name="date-create" id="date-create" >
                    </select>
                </div>
                <span id="ageRatio">Коеффициент возраста - 1</span>
            </div>
            <div class="car-cost">
                <span class="column-title">Стоимость Авто</span>
                <input type="text" name="car-cost" id="car-cost" value="1" disabled/>
                <span id="currentExchange"></span>
            </div>
        </div>
        <div class="extra-charges" style="margin-top: 2rem">
            <div class="extra-charges_item">
                <img src="data:image/png;base64," alt="" class="extra-charges_item__icon">
                <span class="extra-charges_item__name">Сертификация</span>
                <span class="extra-charges_item__cost auction">$ 0</span>
            </div>
            <div class="extra-charges_item" style="margin-bottom: 0">
                <img src="data:image/png;base64," class="extra-charges_item__icon">
                <span class="extra-charges_item__name">МРЭО</span>
                <span class="extra-charges_item__cost" id="MREOCost">$ 0</span>
            </div>

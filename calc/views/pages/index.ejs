    [type="checkbox"]:checked,
    [type="checkbox"]:not(:checked) {
        position: absolute;
        left: -9999px;
    }

    [type="checkbox"]:checked + label,
    [type="checkbox"]:not(:checked) + label {
        position: relative;
        padding-left: 28px;
        cursor: pointer;
        line-height: 20px;
        display: inline-block;
        color: #666;
    }

    [type="checkbox"]:checked + label:before,
    [type="checkbox"]:not(:checked) + label:before {
        content: "";
        position: absolute;
        left: 0;
        top: 0;
        width: 18px;
        height: 18px;
        border: 1px solid #ddd;
        background: #fff;
    }

    [type="checkbox"]:checked + label:after,
    [type="checkbox"]:not(:checked) + label:after {
        content: "";
        width: 8px;
        height: 8px;
        background: #249af3;
        position: absolute;
        top: 6px;
        left: 6px;
        -webkit-transition: all 0.2s ease;
        transition: all 0.2s ease;
    }

    [type="checkbox"]:not(:checked) + label:after {
        opacity: 0;
        -webkit-transform: scale(0);
        transform: scale(0);
    }

    [type="checkbox"]:checked + label:after {
        opacity: 1;
        -webkit-transform: scale(1);
        transform: scale(1);
    }

    /* LOGOUT */
    .logout{
        position: absolute;
        top: 25px;
        right: 25px;
    }
    .logout > a{
        display: block;
        text-decoration: none;
        color: black;
        font-family: Arial, Helvetica, sans-serif;
        font-weight: 600;
        padding: 5px 10px;
        border:  solid 2px #2c2c2c;
        border-radius: 10px;
        cursor: pointer;
        transition: .3s;
    }
    .logout > a:hover{
        color: white;
        background-color: #2c2c2c;
    }
    #preloader{
        width: 100%;
        height: 100vh;
        position: fixed;
        top: 0;
        left: 0;
        background-color: white;
        display: flex;
        justify-content: center;
        align-items: center;
    }
    #preloader > b{
        font-size: 2em;
        font-family: Arial, Helvetica, sans-serif;
        color: #272727;
        animation: pulse 3s infinite alternate-reverse;
    }

    @keyframes pulse{
        50%{
            transform: scale(1.1);
        }
        100%{
            transform: scale(1);
        }
    }

    @media (max-width: 1200px) {
        .calculators {
            align-items: center;
            flex-direction: column;
        }

        .calculators .calculator {
